-- Script Anti Cooldown (Cooldown diatur ke 0.6)
local mt = getrawmetatable(game) -- Mengambil metatable game
setreadonly(mt, false) -- Membuat metatable bisa dimodifikasi
local oldIndex = mt.__index -- Menyimpan fungsi asli __index

-- Menimpa fungsi __index untuk mengatur cooldown ke 0.6
mt.__index = function(self, key)
    if tostring(self) == "shared" and key == "cooldowns" then
        -- Mengatur semua cooldown ke 0.6
        return setmetatable({}, {
            __index = function(_, k)
                return 0.6 -- Cooldown default menjadi 0.6
            end
        })
    elseif tostring(self) == "shared" and key == "multipliers" then
        -- Memastikan multipliers tidak memiliki cooldown lebih tinggi dari 0.6
        return setmetatable({ DisableTagging = false, TagCooldown = 0.6, EnableParrying = true }, {
            __index = function(_, k)
                return rawget(_, k) or 0.6 -- Cooldown default menjadi 0.6
            end
        })
    end
    return oldIndex(self, key)
end

-- Mengatur cooldown fungsi `InCooldown` menjadi 0.6 jika dipanggil
hookfunction(require(game.ReplicatedFirst.Utils).InCooldown, function(action)
    return 0.6 -- Mengembalikan cooldown menjadi 0.6
end)

-- Menampilkan notifikasi
print("fast tag active")
