local Tab = Window:CreateTab("Mainz", 4483362458) -- Title, Image
local Section = Tab:CreateSection("auto")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local trackedTeams = {}
local autotagEnabled = false
local teamCheckEnabled = false
local legitTagEnabled = false

-- Fungsi untuk memperbarui data tim berdasarkan folder PlayerHighlights
local function updateTrackedTeams()
    local playerHighlightsFolder = workspace:FindFirstChild("PlayerHighlights")
    if playerHighlightsFolder then
        -- Reset data tim
        trackedTeams = {}

        -- Tambahkan semua pemain di folder ke tabel trackedTeams
        for _, child in pairs(playerHighlightsFolder:GetChildren()) do
            if child:IsA("Model") then
                trackedTeams[child.Name] = true
            end
        end

        -- Pantau penambahan pemain baru ke folder
        playerHighlightsFolder.ChildAdded:Connect(function(child)
            if child:IsA("Model") then
                trackedTeams[child.Name] = true
            end
        end)

        -- Pantau penghapusan pemain dari folder
        playerHighlightsFolder.ChildRemoved:Connect(function(child)
            if child:IsA("Model") then
                trackedTeams[child.Name] = nil
            end
        end)
    end
end

-- Jalankan pembaruan awal jika folder PlayerHighlights sudah ada
if workspace:FindFirstChild("PlayerHighlights") then
    updateTrackedTeams()
end

-- Pantau jika folder PlayerHighlights muncul setelah game dimulai
workspace.ChildAdded:Connect(function(child)
    if child.Name == "PlayerHighlights" and child:IsA("Folder") then
        updateTrackedTeams()
    end
end)

-- Fungsi untuk memeriksa tim pemain berdasarkan trackedTeams
local function checkTeam(targetPlayer)
    return trackedTeams[targetPlayer.Name] or false
end

-- Fungsi untuk mendapatkan pemain terdekat
local function getClosestPlayer()
    local closestPlayer = nil
    local shortestDistance = math.huge
    local maxDistance = legitTagEnabled and 7 or 9 -- Jarak maksimum berdasarkan Legit Tag

    local localCharacter = LocalPlayer.Character
    if not localCharacter or not localCharacter:FindFirstChild("HumanoidRootPart") then
        return nil
    end
    local localPosition = localCharacter.HumanoidRootPart.Position

    for _, targetPlayer in pairs(Players:GetPlayers()) do
        if targetPlayer ~= LocalPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local targetPlayerRole = targetPlayer:FindFirstChild("PlayerRole")
            if not (targetPlayerRole and targetPlayerRole.Value == "Dead") then
                local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
                local distance = (localPosition - targetPosition).Magnitude

                if distance <= maxDistance then
                    if not teamCheckEnabled or checkTeam(targetPlayer) then
                        closestPlayer = targetPlayer
                        shortestDistance = distance
                    end
                end
            end
        end
    end

    return closestPlayer
end

-- Fungsi untuk melakukan fire langsung ke pemain terdekat
local function autoFireClosestPlayer()
    local closestPlayer = getClosestPlayer()
    if closestPlayer and closestPlayer.Character and closestPlayer.Character:FindFirstChild("Humanoid") then
        local args = {
            [1] = closestPlayer.Character.Humanoid,
            [2] = closestPlayer.Character.HumanoidRootPart.Position
        }

        -- Fire tag pemain terdekat
        ReplicatedStorage.Events.game.tags.TagPlayer:InvokeServer(unpack(args))
        print("Tagged:", closestPlayer.Name)
        wait()
    end
end

-- Toggle Auto Tag
local Toggle1 = Tab:CreateToggle({
    Name = "Auto tag",
    CurrentValue = false,
    Flag = "AutoTagToggle",
    Callback = function(Value)
        autotagEnabled = Value
        if Value then
            task.spawn(function()
                while autotagEnabled do
                    local gamemodeValue = ReplicatedStorage:FindFirstChild("Values")
                    local gamemode = gamemodeValue and gamemodeValue:FindFirstChild("Gamemode") and gamemodeValue.Gamemode.Value
                    local myPlayerRole = LocalPlayer:FindFirstChild("PlayerRole") and LocalPlayer.PlayerRole.Value

                    if gamemode == "Voting" or gamemode == "Votings" or gamemode == "Intermission" then
                        task.wait(1)
                    else
                        if myPlayerRole == "Dead" and not (gamemode == "Chaos" or gamemode == "Crown" or gamemode == "SoloCrown") then
                            task.wait(1)
                        else
                            autoFireClosestPlayer()
                            local waitTime = legitTagEnabled and math.random(2, 3) * 0.1 or 0.3
                            task.wait(waitTime)
                        end
                    end
                end
            end)
        end
    end
})

-- Toggle Team Check
